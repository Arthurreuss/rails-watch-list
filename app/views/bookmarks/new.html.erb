<%= link_to "Go Back to List", list_path(@list), class: "btn btn-primary"%>
<%= simple_form_for [@list, @bookmark] do |f| %>

  <div class="form-inputs">
    <%= f.input :comment %>
    <%= f.input :movie_id, collection: Movie.all - @list.movies, input_html: {data: {controller: "tom_select"}} %>
    <%= f.input :img_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>
